# Meta-Googler Environment Configuration
# Copy to .env and fill in your API keys

# === LOCAL LLM (Free) ===
OLLAMA_BASE_URL=http://localhost:11434
LLM_MODEL=ollama/mistral

# === CLOUD LLM APIS (Optional) ===
# OpenAI / GPT-4
OPENAI_API_KEY=sk-your-key-here

# Anthropic / Claude
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Google / Gemini
GOOGLE_API_KEY=your-key-here

# HuggingFace
HUGGINGFACEHUB_API_TOKEN=hf_your-token-here

# === MUSIC APIs ===
# AudD - Song Identification (Free tier available)
AUDD_API_TOKEN=your-token-here

# ACRCloud - Alternative song identification
ACRCLOUD_ACCESS_KEY=your-access-key
ACRCLOUD_ACCESS_SECRET=your-access-secret
ACRCLOUD_HOST=identify-us-west-2.acrcloud.com

# === COVER ART APIs ===
# Spotify (for cover art)
SPOTIFY_CLIENT_ID=your-client-id
SPOTIFY_CLIENT_SECRET=your-client-secret

# Last.fm
LASTFM_API_KEY=your-api-key

# === APPLICATION ===
LOG_LEVEL=INFO
DEBUG=false
